package  {	import flash.display.MovieClip;	import flash.events.MouseEvent;		public class SubMenuBtn extends MovieClip	{		public function SubMenuBtn() 		{			this.stop();			this.mouseChildren = false;			this.addEventListener(MouseEvent.MOUSE_OVER, over);			this.addEventListener(MouseEvent.MOUSE_OUT, out);		}				private function over(e:MouseEvent):void		{			//trace("over");			this.gotoAndStop("over");		}				private function out(e:MouseEvent):void		{			//trace("out");			this.gotoAndStop("up");		}	}	}