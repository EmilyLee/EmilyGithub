package  {	import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.events.Event;		public class mcJourneyPopup extends MovieClip	{		private var _toggle:Boolean = false;						public function mcJourneyPopup() 		{			hit.addEventListener(MouseEvent.CLICK, toggleMenu);		}				public function get onOff():Boolean		{			return _toggle;		}				public function open():void		{			_toggle = true;			this.gotoAndStop(2);		}				public function close():void		{			_toggle = false;			this.gotoAndStop(1);		}				private function toggleMenu(e:MouseEvent):void		{			dispatchEvent(new Event("clickPopup"));						/*if(this.currentFrame == 1)			{				open();			}			else			{				close();			}*/		}			}	}